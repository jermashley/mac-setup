{
    "[typescriptreact]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "beautify.ignore": [
        "**/*.js",
        "**/*.jsx"
    ],
    "blade.format.enable": true,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".eslintrc.js",
        ".vscode",
        "typings"
    ],
    "cSpell.language": "en",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.attachShellCommand.linuxContainer": "bash",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorWidth": 2,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "'FuraCode Nerd Font', 'Fira Code', 'Input Mono', 'Source Code Pro', 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "700",
    "editor.formatOnSave": true,
    "editor.letterSpacing": 0.65,
    "editor.lineHeight": 38,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.quickSuggestionsDelay": 50,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "all",
    "editor.suggestFontSize": 16,
    "editor.suggestLineHeight": 38,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "emmet.includeLanguages": {
        "blade": "html",
        "blade.php": "html",
        "css": "css",
        "javascript": "javascriptreact",
        "jsx": "javascriptreact",
        "php": "html",
        "php-html": "html",
        "tsx": "typescriptreact",
        "twig": "html",
        "typescript": "typescriptreact",
        "vue": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "vue-html"
    ],
    "explorer.openEditors.visible": 0,
    "files.associations": {
        "*.blade.php": "blade",
        "*.js": "javascriptreact",
        "*.jsx": "javascriptreact",
        "*.php": "php",
        ".php_cs": "php",
        ".php_cs.dist": "php"
    },
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "html-css-class-completion.enableEmmetSupport": true,
    "html-css-class-completion.includeGlobPattern": "**/*.{css,scss,html}",
    "importCost.typescriptExtensions": [
        "\\.vue$"
    ],
    "javascript.format.enable": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "materialTheme.accent": "Purple",
    "peacock.affectActivityBar": false,
    "peacock.affectStatusBar": true,
    "peacock.affectTitleBar": true,
    "php-docblocker.qualifyClassNames": true,
    "php-docblocker.returnGap": true,
    "php.suggest.basic": false,
    "prettier.eslintIntegration": true,
    "prettier.jsxBracketSameLine": true,
    "prettier.jsxSingleQuote": true,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.stylelintIntegration": true,
    "prettier.trailingComma": "es5",
    "references.preferredLocation": "view",
    "sync.askGistName": false,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "sync.gist": "63b83101c66de20e95549a6e7d7e8de7",
    "sync.quietSync": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "telemetry.enableTelemetry": false,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.fontFamily": "'FuraCode Nerd Font', 'Fira Code', 'Input Mono', 'Source Code Pro', 'Courier New', monospace",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.rendererType": "canvas",
    "vetur.validation.template": true,
    "window.clickThroughInactive": false,
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#AB47BC",
        "editorSuggestWidget.highlightForeground": "#AB47BC",
        "editorWidget.border": "#AB47BC",
        "list.activeSelectionForeground": "#AB47BC",
        "list.highlightForeground": "#AB47BC",
        "list.inactiveSelectionForeground": "#AB47BC",
        "notificationLink.foreground": "#AB47BC",
        "pickerGroup.foreground": "#AB47BC",
        "progressBar.background": "#AB47BC",
        "scrollbarSlider.activeBackground": "#AB47BC50",
        "tab.activeBorder": "#AB47BC",
        "textLink.foreground": "#AB47BC"
    },
    "workbench.colorTheme": "Noctis Uva",
    "workbench.editor.showTabs": true,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "newUntitledFile"
}